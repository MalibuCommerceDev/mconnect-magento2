                </field>
                <field id="set_in_stock" sortOrder="70" type="select" showInDefault="1" showInWebsite="1" showInStore="0" translate="label">
                    <label>Set Product Stock Status to "In Stock" if QTY > "0"</label>
                    <source_model>\Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="disable_new_products" sortOrder="80" type="select" showInDefault="1" showInWebsite="1" showInStore="0" translate="label">
                    <label>Disable New Imported Products</label>
                    <source_model>\Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
            </group>

            <group id="inventory" type="text" sortOrder="70" showInDefault="1" showInWebsite="1" showInStore="0" translate="label">
