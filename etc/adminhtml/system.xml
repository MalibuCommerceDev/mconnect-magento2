                <field id="import_enabled" sortOrder="5" type="select" showInDefault="1" showInWebsite="1" showInStore="0" translate="label">
                    <label>Import Enabled</label>
                    <source_model>\Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="export_enabled" sortOrder="10" type="select" showInDefault="1" showInWebsite="1" showInStore="0" translate="label">
                    <label>Export Enabled</label>
                    <source_model>\Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="hold_new_orders_export" sortOrder="20" type="select" showInDefault="1" showInWebsite="1" showInStore="0" translate="label">
                    <label>Hold New Orders Export</label>
                    <source_model>\Magento\Config\Model\Config\Source\Yesno</source_model>
                    <frontend_model>\MalibuCommerce\MConnect\Block\Config\Frontend\Field\HoldNewExport</frontend_model>
                </field>
                <field id="hold_new_orders_delay" sortOrder="30" type="text" showInDefault="1" showInWebsite="1" showInStore="0" translate="label">
                    <label>New Orders Sync Delay</label>
                    <validate>validate-number validate-zero-or-greater</validate>
