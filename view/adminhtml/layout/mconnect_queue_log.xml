<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="styles"/>
    <body>
        <referenceBlock name="menu">
            <action method="setActive">
                <argument name="itemId" xsi:type="string">MalibuCommerce_MConnect::queue</argument>
            </action>
        </referenceBlock>
        <referenceBlock name="page.title">
            <action method="setTitleClass">
                <argument name="class" xsi:type="string">complex</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="MalibuCommerce\MConnect\Block\Adminhtml\Queue\Log" name="mconnect.log" template="MalibuCommerce_MConnect::queue/log.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>